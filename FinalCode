[TestClass]
public class CanDriveTests
{
    [TestMethod]
    public void testMethod1()
    {
        Assert.IsFalse(CanDriveMethod.CanDrive(int.MinValue));
    }
    [TestMethod]
    public void testMethod2()
    {
        Assert.IsFalse(CanDriveMethod.CanDrive(int.MinValue + 1));
    }
    [TestMethod]
    public void testMethod3()
    {
        Assert.IsFalse(CanDriveMethod.CanDrive(15));
    }
    [TestMethod]
    public void testMethod4()
    {
        Assert.IsTrue(CanDriveMethod.CanDrive(16));
    }
    [TestMethod]
    public void testMethod5()
    {
        Assert.IsTrue(CanDriveMethod.CanDrive(17));
    }
    [TestMethod]
    public void testMethod6()
    {
        Assert.IsTrue(CanDriveMethod.CanDrive(int.MaxValue - 1));
    }
    [TestMethod]
    public void testMethod7()
    {
        Assert.IsTrue(CanDriveMethod.CanDrive(int.MaxValue));
    }
    
}
public static class CanDriveMethod
{
    public static bool CanDrive(int age)
    {
        const int drivingAge = 16;
        return age >= drivingAge;
    }
}
[TestClass]
public class LoginTest
{
    private static IWebDriver driver = new ChromeDriver();
    [TestMethod]
    public void successfulTest()
    {
        
        driver.Navigate().GoToUrl("https://www.yourlearningpal.com/login");
        
        var loginURL = driver.Url;
        
        var user = driver.FindElement(By.Id("txtUser"));
        user.Click();
        user.Clear();
        user.SendKeys("test1");
    
        
        var password = driver.FindElement(By.Id("txtPassword"));
        password.Click();
        password.Clear();
        password.SendKeys("Test12456");
        
        var login = driver.FindElement(By.CssSelector("button[type='submit']"));
        login.Click();
        
        Assert.AreNotEqual(loginURL, driver.Url);
        
        

    
        
    }
    [TestMethod]
    public void unsuccessfulTest()
    {
        
        driver.Navigate().GoToUrl("https://www.yourlearningpal.com/login");
        var loginURL = driver.Url;
        
        var user = driver.FindElement(By.Id("txtUser"));
        user.Click();
        user.Clear();
        user.SendKeys("test1");
    
        
        var password = driver.FindElement(By.Id("txtPassword"));
        password.Click();
        password.Clear();
        password.SendKeys("test1234");
        
        var login = driver.FindElement(By.CssSelector("button[type='submit']"));
        login.Click();
        
        Assert.AreEqual(loginURL, driver.Url);
    
    
        
    }
}

