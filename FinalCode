[TestClass]
public class CanDriveTests
{
    [Test]
    public void testMethod1()
    {
        assert.isfalse(CanDriveMethod.CanDrive(0));
    }
    [Test]
    public void testMethod2()
    {
        assert.isfalse(CanDriveMethod.CanDrive(3));
    }
    [Test]
    public void testMethod3()
    {
        assert.isfalse(CanDriveMethod.CanDrive(15));
    }
    [Test]
    public void testMethod3()
    {
        assert.istrue(CanDriveMethod.CanDrive(16));
    }
    [Test]
    public void testMethod3()
    {
        assert.istrue(CanDriveMethod.CanDrive(17));
    }
    [Test]
    public void testMethod3()
    {
        assert.istrue(CanDriveMethod.CanDrive(50));
    }
    
}
public class CanDriveMethod
{
    public bool CanDrive(int age) {

    const int drivingAge = 16; return age >= drivingAge;

}
public class LoginTest
{
    private static IWebDriver driver = new ChromeDriver();
    
      public void successfulTest()
    {
        
        driver.Navigate().GoToUrl("https://www.yourlearningpal.com/login");
      
        var user = driver.FindElement(By.Id("txtUser"));
        user.Click();
        user.Clear();
        user.SendKeys("test1");
    
      
        var password = driver.FindElement(By.Id("txtPassword"));
        password.Click();
        password.Clear();
        password.SendKeys("Test12456");
        
        var login = driver.FindElement(By.CssSelector("button[type='submit']"));
        login.Click();

    
        
    }
    
     public void unsuccessfulTest()
    {
       
        driver.Navigate().GoToUrl("https://www.yourlearningpal.com/login");
     
        var user = driver.FindElement(By.Id("txtUser"));
        user.Click();
        user.Clear();
        user.SendKeys("test1");
    
       
        var password = driver.FindElement(By.Id("txtPassword"));
        password.Click();
        password.Clear();
        password.SendKeys("test1234");
     
        var login = driver.FindElement(By.Id("button"));
        login.Click();
    
        
    }
}

